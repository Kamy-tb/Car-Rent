<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/graph"
    app:startDestination="@id/voituresFragment2">

    <fragment
        android:id="@+id/voituresFragment2"
        android:name="com.example.tp4_1.VoituresFragment"
        android:label="fragment_voitures"
        tools:layout="@layout/fragment_voitures" >
        <action
            android:id="@+id/action_voituresFragment2_to_detailFragment"
            app:destination="@id/detailFragment" />
    </fragment>
    <fragment
        android:id="@+id/detailFragment"
        android:name="com.example.tp4_1.DetailFragment"
        android:label="DetailFragment" />
    <fragment
        android:id="@+id/profilFragment2"
        android:name="com.example.tp4_1.ProfilFragment"
        android:label="fragment_profil"
        tools:layout="@layout/fragment_profil" >
        <action
            android:id="@+id/action_profilFragment2_to_changePwdFragment"
            app:destination="@id/changePwdFragment" />
    </fragment>
    <fragment
        android:id="@+id/trajetsFragment2"
        android:name="com.example.tp4_1.TrajetsFragment"
        android:label="fragment_trajets"
        tools:layout="@layout/fragment_trajets" />
    <fragment
        android:id="@+id/reservationFragment2"
        android:name="com.example.tp4_1.ReservationFragment"
        android:label="fragment_reservation"
        tools:layout="@layout/fragment_reservation" />
    <fragment
        android:id="@+id/changePwdFragment"
        android:name="com.example.tp4_1.ChangePwdFragment"
        android:label="fragment_change_pwd"
        tools:layout="@layout/fragment_change_pwd" >
        <action
            android:id="@+id/action_changePwdFragment_to_profilFragment2"
            app:destination="@id/profilFragment2" />
    </fragment>
</navigation>